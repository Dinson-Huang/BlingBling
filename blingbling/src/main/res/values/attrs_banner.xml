<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BannerView">
        <attr name="open_mz_mode" format="boolean"/>
        <attr name="bannerAspectRatio" format="float"/>
        <attr name="canLoop" format="boolean"/>
        <attr name="autoLoop" format="boolean"/>
        <attr name="hiddenIndicator" format="boolean"/>
        <attr name="indicatorMargin" format="dimension"/>
        <attr name="indicatorMarginBottom" format="dimension"/>
        <attr name="indicatorAlign">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="middle_page_cover" format="boolean"/>
    </declare-styleable>
</resources>
